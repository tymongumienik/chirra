generator client {
  provider = "prisma-client-js"
  output   = "../../../node_modules/.prisma/client"
}

datasource db {
  provider = "postgresql"
}

model User {
  id          String   @id @default(cuid())
  email       String   @unique
  username    String   @unique
  displayname String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
